pico-8 cartridge // http://www.pico-8.com
version 36
__lua__
function _init()
 -- enable mouse devkit
	poke(0x5f2d, 1)
end

function _draw()
 -- clean the screen
	cls()
	
	-- print mouse coordinates
	print("x: "..stat(32))
	print("y: "..stat(33))

	-- check if button pressed
	curbut=stat(34)
	but=""
	if curbut == 0x1
	then but = "left"
	elseif curbut == 0x2
	then but = "right"
	elseif curbut == 0x4
	then but = "middle"
	else but = "none"
	end
	print("button: "..but.." ("..curbut..")")

	-- show a literal mouse
	spr(0,stat(32)-1,stat(33)-1)
end
